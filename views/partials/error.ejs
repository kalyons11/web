<div id="mainRow" class="row">
	<div class="col-sm-8">
		<h1>Whoops.</h1>
		<h3><p class="text-muted">Something went wrong.</p></h3>
		<hr>
		<% if (model.error.code == 404) { %>
			<p>We were unable to find the requested resource at <%= model.error.url %>. Please double check your URL, or simply head <a href="/">home</a>.</p>
		<% } else if (model.error.code == 401) { %>
			<p>You do not have permission to access <%= model.error.url %> on this server. Please head back <a href="/">home</a>.</p>
		<% } else if (model.error.code == 500) { %>
			<p>An internal server error occurred while trying to access <%= model.error.url %>. Please head back <a href="/">home</a>.</p>
		<% } %>
	</div>
	<div id="rightColumn" class="col-sm-2">
		<% include ./rightBar %>
	</div>
</div>